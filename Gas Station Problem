def canCompleteCircuit(gas, cost):
    total, tank, start = 0, 0, 0
    for i in range(len(gas)):
        tank += gas[i] - cost[i]
        total += gas[i] - cost[i]
        if tank < 0:
            start = i + 1
            tank = 0
    return start if total >= 0 else -1

gas = [4, 5, 7, 4]
cost = [6, 6, 3, 5]
print(canCompleteCircuit(gas, cost))  
